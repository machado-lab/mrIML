Package: mrIML
Type: Package
Title: Multi Response Interpretable Machine Learning
Version: 0.1
Authors@R: c(person("Nick","Fountain-Jones", email = "nfountainjones@email.com", role = c("aut", "cre"),
                     comment = c(ORCID = "0000-0001-9248-8493")),
